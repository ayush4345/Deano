{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/tasks/components/columns.jsx", "src/app/tasks/components/data-table-column-header.jsx", "src/app/tasks/components/data-table-faceted-filter.jsx", "src/app/tasks/components/data-table-pagination.jsx", "src/app/tasks/components/data-table-row-actions.jsx", "src/app/tasks/components/data-table-toolbar.jsx", "src/app/tasks/components/data-table-view-options.jsx", "src/app/tasks/components/data-table.jsx", "src/app/tasks/components/user-nav.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
